EXE=tokenizer
SO=$(EXE).so
#all: exe so

#exe:
#	$(CC) $(COMMON) $(SRC) $(INC) $(LIBS) /usr/lib64/python2.6/config/libpython2.6.so -o $(EXE)
so:
	$(CC) $(COMMON) $(SRC) $(INC) $(LIBS)  -o $(SO) -shared 
